{
    "regex": [
    {
      "field":"title",
      "find":"([0-9]{3}\\s)?(([0-9]{3,4})|ELEMENTARY)(?! Room)(?! Bldg:)(?!\\s*[0-9]*\\n) ((?! [A-Z][a-z])[A-Z0-9,-:& \\/\\'])*( Lecture| Recitation| Lab)?",
      "delete":"(?<![A-Za-z])\\s*[0-9]+\\s",
      "substitute":"",
      "required":1,
      "course":1,
      "section":0,
      "capitalize":1
    },
    {
      "field":"subject",
      "find":"(?<![A-Za-z0-9])[A-Z]{3,4}(?=[\\s\\n]+[0-9]{2,3}[A-Z]?)",
      "delete":"",
      "substitute":"",
      "required":1,
      "course":1,
      "section":0,
      "capitalize":0
    },
    {
      "field":"courseNumber",
      "find":"(?<![A-Za-z0-9])[A-Z]{3,4}[\\s\\n][0-9]{2,3}[A-Z]?",
      "delete":"[A-Z]{3,4}[\\s\\n](?=[0-9]{2,3}[A-Z]?)",
      "substitute":"",
      "required":1,
      "course":1,
      "section":0,
      "capitalize":0   
    },
    {
      "field":"attribute",
      "find":"Attributes:\\s*.*(?=\\n).','Attributes:",
      "delete":"",
      "substitute":"",
      "required":0,
      "course":0,
      "section":0,
      "capitalize":0   
    },
    {
      "field":"instructor",
      "find":"[A-Z][A-Za-z\\-\\']+,[A-Z][A-Za-z\\-\\']+|(?<=Instructor:\\s)Staff|(?<=Instructor:\\s)TBA|(?<=ABOUT THE INSTRUCTOR\\:\\s)[A-Z][a-zA-Z\\-]+\\s[A-Z][a-zA-Z\\-]+",
      "delete":"([\\w\\-]+),([\\w\\-]+).*",
      "substitute":"\\2 \\1",
      "required":0,
      "course":0,
      "section":1,
      "capitalize":1
    },
    {
      "field":"description",
      "find":"(ABOUT THE SEMINAR|COURSE DESCRIPTION):[^$]*\\.(?=[^_\\.]*$)",
      "delete":"(ABOUT THE SEMINAR|COURSE DESCRIPTION):|\\s?\\n\\s?|\\xc2\\xbf",
      "substitute":"",
      "required":0,
      "course":1,
      "section":0,
      "capitalize":0
    },
    {
      "field":"time",
      "find":"Time:\\s[0-9]{2}:[0-9]{2}\\s-\\s[0-9]{2}:[0-9]{2}",
      "delete":"Time:|\\s|^\\s+|\\s+$",
      "substitute":"",
      "required":0,
      "course":0,
      "section":0,
      "capitalize":0
    },
    {
      "field":"days",
      "find":"Days:\\s(M|Tu|W|Th|F)+",
      "delete":"Days:|\\s",
      "substitute":"",
      "required":0,
      "course":0,
      "section":0,
      "capitalize":0
    },
    {
      "field":"building",
      "find":"(?<=Bldg:\\s)((?!\\s?Room|\\n|TBA)[\\w ])+",
      "delete":"",
      "substitute":"",
      "required":0,
      "course":0,
      "section":1,
      "capitalize":0
    },
    {
      "field":"room",
      "find":"Room:\\s[0-9]+",
      "delete":"Room:|\\s",
      "substitute":"",
      "required":0,
      "course":0,
      "section":1,
      "capitalize":0
    },
    {
      "field":"courseSection",
      "find":"[A-Z]{3,4}\\s[0-9]{2,3}[A-Z]?\\s[0-9]{3}",
      "delete":"[A-Z]{3,4}\\s[0-9]{2,3}[A-Z]?\\s",
      "substitute":"",
      "required":0,
      "course":0,
      "section":1,
      "capitalize":0
    }
    ]
}